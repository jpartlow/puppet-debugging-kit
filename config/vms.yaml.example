---
vms:

  # NOTE: All `box:` declarations in this example point to vagrantcloud.com
  # and require Vagrant 1.5.0 or newer to function.

  - name: pe-372-master
    debug-kit: true
    box: puppetlabs/centos-6.5-64-nocm
    roles:
      - pl-src-sync
      - el-helper-packages
      - pe-forward-console
      - el-stop-firewall
      - el-fix-path
      - 4gb-memory
      - base

  - name: pe-372-agent
    debug-kit: true
    box: puppetlabs/centos-6.5-64-nocm
    roles:
      - pl-src-sync
      - el-helper-packages
      - el-stop-firewall
      - el-fix-path
      - base

